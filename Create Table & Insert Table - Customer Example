CREATE TABLE customer (
    customer_id     NUMBER,
    customer_name   VARCHAR2(80),
    age             NUMBER,
    gender          VARCHAR2(8),
    phone_number    NUMBER,
    bill_amount     FLOAT
);

# Create Table and assign each variable you want its own variable type, based on use case.

insert into customer (customer_id,customer_name,age,gender,phone_number,bill_amount) VALUES(1,'Ajay',35,'Male','Bangalore',50000);
